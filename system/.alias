# Aliases

# reload
alias reload="source ~/.bash_profile"

# code
alias dev='cd ~/Development/'

# codelog
alias clog='cd ~/Development/codelog'

# desktop
alias desktop='cd ~/Desktop/'

# personal
alias pers='cd ~/Development/personal/'
alias tama='cd ~/Development/personal/tamagotchi'

# pillar
alias pillar='cd ~/Development/pillar/'

# Git
alias g="git"
alias gst="git status"
alias gco="git checkout"
alias gp="git pull"
alias gm="git merge"
alias gd="git diff | mate"
alias gc="git commit -v"
alias gf="git fetch"
alias gci="git commit -m"
alias gca="git commit -v -a"
alias gb="git branch"
alias gba="git branch -a"
alias add="git add ."
alias gbd="git branch -D"
alias gbrd="git branch -r -d"
alias remote="git remote -v"
alias diff="git diff"

